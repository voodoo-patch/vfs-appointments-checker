{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Job": {
    "CronSchedule": " 0 0/10 8-18/1 1/1 * ? *",
    "RunOnStartup": false
  },
  "Telegram": {
    "BotApiEndpoint": "https://api.telegram.org/bot",
    "Channels": [
      "[REPLACE_WITH_YOUR_CHANNEL_ID]"
    ]
  },
  "VfsChecker": {
    "AuthenticationEndpoint": "https://visa.vfsglobal.com/gbr/en/ita/login",
    "Browser": {
      "Headless": false
    },
    "Accounts": [
      {
        "Email": "[REPLACE_WITH_YOUR_USERNAME]",
        "Password": "[REPLACE_WITH_YOUR_PASSWORD]"
      }
    ]
  }
}